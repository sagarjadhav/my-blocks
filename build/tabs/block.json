{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/tabs",
  "version": "0.1.0",
  "title": "Tabs Block",
  "category": "widgets",
  "icon": "smiley",
  "description": "Tabs block scaffolded with Create Block tool.",
  "supports": {
    "html": false,
    "anchor": true
  },
  "textdomain": "my-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "uniqueId": {
      "type": "string"
    },
    "tabLabelsArray": {
      "type": "array",
      "default": []
    },
    "updateChild": {
      "type": "boolean",
      "default": false
    }
  }
}