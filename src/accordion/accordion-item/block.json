{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/accordion-item",
	"version": "0.1.0",
	"title": "Accordion Item",
	"category": "widgets",
	"icon": "smiley",
	"parent": [ "create-block/accordion" ],
	"description": "Accordion Item block scaffolded with Create Block tool.",
	"supports": {
		"html": false
	},
	"textdomain": "my-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"title": {
			"type": "string",
			"default": ""
		},
		"isOpen": {
			"type": "boolean",
			"default": false
		}
	}
}
